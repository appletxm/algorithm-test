{
  "name": "algorithm-test",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "eslint": "eslint --config .eslintrc.js src/js/**/*.js",
    "start": "npm run dev",
    "dev": "concurrently --kill-others \"npm run babel-watch\" \"npm run server\"",
    "babel": "babel src --out-dir dist --source-maps inline",
    "babel-watch": "babel src --out-dir dist --watch",
    "server": "node scripts/server.js",
    "debug": "concurrently --kill-others \"npm run babel-watch\" \"node --inspect scripts/server.js\"",
    "debugJWT": "node --inspect scripts/jwt-server.js"
  },
  "dependencies": {
    "express": "^4.17.1",
    "http-proxy-middleware": "^2.0.1",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/plugin-transform-spread": "^7.8.3",
    "@babel/preset-env": "^7.6.3",
    "babel-eslint": "^10.1.0",
    "chalk": "2.3.0",
    "concurrently": "^3.5.1",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-standard": "3.0.1",
    "event-source-polyfill": "0.0.12",
    "open": "0.0.5",
    "ora": "1.3.0",
    "rimraf": "2.6.2"
  },
  "nodejs": {
    "version": ">=8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/appletxm/es6-practise"
  },
  "keywords": [
    "es6",
    "node"
  ],
  "author": "appletxm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/appletxm/es6-practise/issues"
  }
}
